<!--pages/result/result.wxml-->
<view wx:for="{{resultsArray}}">
  <block wx:if="{{item.timestamp==timestamp}}">
    <view class='wrapper'>
      <view class='header_blk'>
        <image class='header_blk' src='../../common/image/zwb_logo_01.jpg'></image>
      </view>
      <block wx:if="{{item.typeValue=='alarm'}}">
        <view class='wrapper'>
          <text class='area_tit'>告警数据</text>
          <image class='header_tit' src='../../common/image/title_underline.png'></image>
          <view wx:for="{{item.result}}" wx:for-index="idx" wx:for-item="line">

            <view wx:for="{{line}}" wx:for-index="lineNum" wx:for-item="content">

              <view wx:if="{{lineNum==0}}" class='line'>
                <view class='title'>
                  <text>序号</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==1}}" class='line'>
                <view class='title'>
                  <text>网元名称</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==2}}" class='line'>
                <view class='title'>
                  <text>所属区县</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==3}}" class='line'>
                <view class='title'>
                  <text>设备厂家</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==4}}" class='line'>
                <view class='title'>
                  <text>告警发生时间</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==5}}" class='line'>
                <view class='title'>
                  <text>厂家告警号</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==6}}" class='line'>
                <view class='title'>
                  <text>告警中文名</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==7}}" class='line'>
                <view class='title'>
                  <text>告警对象名称</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==8}}" class='line'>
                <view class='title'>
                  <text>基站编号</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

              <view wx:if="{{lineNum==9}}" class='line'>
                <view class='title'>
                  <text>基站名称</text>
                </view>
                <view class='content'>
                  <text>{{content}}</text>
                </view>
              </view>

            </view>
            <!--空行-->
            <view><text class='blank'></text></view>
          </view>
        </view>

      </block>
    </view>
  </block>

</view>
